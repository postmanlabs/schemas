<!DOCTYPE html>
<html>
  <head>
      <title>Postman Collection Format {{ version }} Schema Documentation</title>
      <meta charset="utf-8">
      <link rel="shortcut icon" href="https://www.getpostman.com/favicon.ico">
      <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width">
      <link rel="stylesheet" href="{{{root}}}/css/docson.css">
      <link rel="stylesheet" type="text/css" href="{{{root}}}/css/styles.css">
      <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.5/css/bootstrap.min.css" />
      <link href='https://fonts.googleapis.com/css?family=Open+Sans:300,600' rel='stylesheet' type='text/css'>

      <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
      <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
      <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
      <![endif]-->

      <script src="{{{root}}}/lib/require.js"></script>
      <script type="text/javascript">
          if (document.location.hostname.search("localhost") !== 'localhost') {
              (function (i, s, o, g, r, a, m) {
                  i['GoogleAnalyticsObject'] = r;
                  i[r] = i[r] || function () {
                      (i[r].q = i[r].q || []).push(arguments)
                  }, i[r].l = 1 * new Date();
                  a = s.createElement(o),
                      m = s.getElementsByTagName(o)[0];
                  a.async = 1;
                  a.src = g;
                  m.parentNode.insertBefore(a, m)
              })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
              ga('create', 'UA-43979731-4', 'auto');
              ga('send', 'pageview');
          }
      </script>
  </head>
  <body>
      <div id="header" class="header-container clearfix hero-header-container background-dark open-sans">
          <a href="https://www.getpostman.com/" class="logo-container margin-top-5"></a>
          <div class="navigation-container">
              <ul>
                  <li><a href="https://www.getpostman.com" class="btn pm-btn-transparent">Get Postman</a></li>
              </ul>
          </div>
      </div>
      <div class="page-wrap">
          <div id="content" class="container">
              <div>
                  <div class="row version-header">
                      <h3>Postman Collection Format {{ version }}</h3>
                      <label class="right"><input type="checkbox" checked="checked" id="show-internal-check" /> Show
                          Reserved
                          Keywords</label>
                  </div>
                  <div>
                      <div id="doc"></div>
                  </div>
                  <script charset="utf-8">
                      require(["{{{root}}}/lib/docson", "{{{root}}}/lib/main", "{{{root}}}/lib/jquery"], function (docson, main) {
                          $(function () {
                              var schema;
                              $.get('../collection.json', function (response) {
                                  schema = response;
                                  docson.templateBaseUrl = "{{{root}}}/templates";
                                  docson.doc("doc", schema);
                              });

                              $('#show-internal-check').change(function () {
                                  var unchecked = !this.checked;
                                  $.get('../collection.json', function (schema) {
                                      docson.templateBaseUrl = "{{{root}}}/templates";
                                      if (unchecked) {
                                          main.removePrivate(schema);
                                      }
                                      console.log(JSON.stringify(schema, null, 2));
                                      docson.doc("doc", schema);
                                  });
                              });
                          });
                      });

                  </script>
              </div>
              <div>
                  <h3>Legend</h3>
                  <table class="table table-striped">
                      <tbody>
                          <tr>
                              <td><span class="required">propertyName</span></td>
                              <td>A Required property.</td>
                          </tr>
                          <tr>
                              <td><span class="signature-type-string"><strong>dataType</strong></span></td>
                              <td>Denotes the data type of a property, e.g "string" or "number".</td>
                          </tr>
                          <tr>
                              <td><span class="type-enum"><strong>variableValue</strong></span></td>
                              <td>One of a few valid values.</td>
                          </tr>
                      </tbody>
                  </table>
              </div>
              <div>
                  <p>
                      The raw schema is located at
                      <a href="http://schema.getpostman.com/collection/json/{{ version }}/{{ draft }}/collection.json" target=“_blank”>
                          http://schema.getpostman.com/collection/json/{{ version }}/{{ draft }}/collection.json
                      </a>
                  </p>
              </div>
          </div>
      </div>
      <div class="footer-container open-sans">
          <span>&copy;
            <script type="text/JavaScript">document.write(new Date().getFullYear());</script>
            Postdot Technologies, Inc.
          </span>
          <span>All Rights Reserved</span>
      </div>
  </body>
</html>
